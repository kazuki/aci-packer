{
  "name": "aci.oikw.org/python",
  "labels": [
    { "name": "version", "value": "3.3.5" }
  ],
  "-aci-packer-build-steps-": [
    {
      "step": "symlink",
      "links": [
        ["lib", "/usr/lib64"],
        ["/bin/env", "/usr/bin/env"],
        ["python3.3", "/usr/bin/python3"],
        ["python3.3", "/usr/bin/python"]
      ]
    },
    {
      "step": "copy",
      "binaries": [
        "/usr/bin/python3.3",
        "/bin/env",
        "/bin/busybox"
      ],
      "find_executable": [
        "/usr/lib/python3.3/"
      ],
      "files": [
        ["/usr/lib/python3.3/", "/usr/lib/python3.3/"],
        ["/usr/include/python3.3/", "/usr/include/python3.3/"]
      ],
      "excludes": [
        "/usr/lib/python3.3/ctypes/test",
        "/usr/lib/python3.3/lib2to3/tests",
        "/usr/lib/python3.3/test",
        "/usr/lib/python3.3/unittest/test",
        "/usr/lib/python3.3/distutils/tests",
        "/usr/lib/python3.3/site-packages",
        "/usr/lib/python3.3/sqlite3/test"
      ]
    },
    {
      "step": "setup_chroot"
    },
    {
      "step": "cmd",
      "path": "/bin/busybox",
      "args": [
        "--install",
        "-s",
        "/bin"
      ]
    },
    {
      "step": "write",
      "path": "/etc/passwd",
      "contents": "root:x:0:0:root:/:/bin/sh\n"
    },
    {
      "step": "write",
      "path": "/etc/group",
      "contents": "root:x:0:root\n"
    },
    {
      "step": "shell",
      "cmd": "wget -O - https://bootstrap.pypa.io/get-pip.py | chroot $ROOTFS /usr/bin/python"
    },
    {
      "step": "delete",
      "files": [
        "/pip_build_root",
        "/.cache",
        "/root"
      ]
    }
  ]
}
